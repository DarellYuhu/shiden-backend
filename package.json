{
  "name": "shiden-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "bun build src/main -e @nestjs/microservices -e class-validator -e class-transformer -e @nestjs/core -e ts-morph --target bun --outdir dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:novu": "bunx novu@latest dev -p 3000 -d https://nv.binili.net",
    "start:dev": "bun --watch run src/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@novu/api": "^1.7.1",
    "@prisma/client": "^6.17.1",
    "@scalar/nestjs-api-reference": "^1.0.5",
    "@thallesp/nestjs-better-auth": "^2.1.0",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "better-auth": "^1.3.27",
    "convert-csv-to-json": "^3.11.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "file-type": "^21.0.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.6",
    "nestjs-zod": "^5.0.1",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "web-push": "^3.6.7",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.5",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.3.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.10",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
